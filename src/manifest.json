{
  "name": "AmazonBedrockForZendesk",
  "author": {
    "name": "Sean Ye",
    "email": "yexw@amazon.com",
    "url": "https://www.zendesk.com"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": {
        "url": "assets/iframe.html",
        "flexible": true
      }
    }
  },
  "domainWhitelist": ["ai.plaza.red","apisix-data.hijackery.cn"],
  "parameters": [
    {
      "name": "apiToken",
      "type": "text",
      "secure": true
    },
    {
      "name": "aiServerUrl",
      "type": "text",
      "secure": false
    },
    {
      "name": "prompt",
      "type": "multiline",
      "secure": false
    }
  ],
  "version": "1.0",
  "frameworkVersion": "2.0"
}
